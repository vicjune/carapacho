{
  "name": "carapacho",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_FB_EMULATORS=true vite",
    "dev:prod": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "unused-exports": "ts-unused-exports ./tsconfig.json --showLineNumber",
    "server": "cd functions && yarn && yarn build && cd .. && firebase emulators:start --import emulatorData --export-on-exit",
    "start-functions": "cd functions && yarn && yarn build:watch",
    "save-indexes": "firebase firestore:indexes > firestore.indexes.json"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.9",
    "@mui/lab": "^6.0.0-beta.21",
    "@mui/material": "^6.1.9",
    "date-fns": "^4.1.0",
    "firebase": "^11.1.0",
    "i18next": "^24.1.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.2.0",
    "react-modal-hook": "^3.0.2",
    "react-router": "^7.0.1",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-checker": "^0.8.0"
  }
}
